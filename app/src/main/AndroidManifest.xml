<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reportapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".network_signuppage.SignupPageActivity" />
        <activity android:name=".network_loginpage.LoginPageActivity" />
        <activity android:name=".network_getreport.MainActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".netnwork_postreport.ReportActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".About" />
        <activity android:name=".ActivitySlidShow"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>